<suite name="TestTangoAdmin" verbose="1">

    <test name="TestChrome">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="tango.pages.Login"/>
            <class name="tango.pages.AdminDashboard"/>
            <class name="tango.Untils"/>
            <class name="tango.TestCaseAdmin"/>
            <class name ="tango.pages.Overview"/>
            <class name="tango.pages.AdminAdviser"/>
            <class name="tango.pages.AdminContractor"/>
            <class name="tango.pages.Messages"/>
            <class name="tango.pages.AddOns"/>
        </classes>
    </test>
    <test name="TestEdge">
        <parameter name ="browser" value="Edge"/>
        <classes>
            <class name="tango.pages.Login"/>
            <class name="tango.pages.AdminDashboard"/>
            <class name="tango.Untils"/>
            <class name="tango.TestCaseAdmin"/>
            <class name ="tango.pages.Overview"/>
            <class name="tango.pages.AdminAdviser"/>
            <class name="tango.pages.AdminContractor"/>
            <class name="tango.pages.Messages"/>
            <class name="tango.pages.AddOns"/>
        </classes>
    </test>
    <test name="TestFireFox">
        <parameter name="browser" value="Firefox"/>
        <classes>
            <class name="tango.pages.Login"/>
            <class name="tango.pages.AdminDashboard"/>
            <class name="tango.Untils"/>
            <class name="tango.TestCaseAdmin"/>
            <class name ="tango.pages.Overview"/>
            <class name="tango.pages.AdminAdviser"/>
            <class name="tango.pages.AdminContractor"/>
            <class name="tango.pages.Messages"/>
            <class name="tango.pages.AddOns"/>
        </classes>
    </test>
</suite>